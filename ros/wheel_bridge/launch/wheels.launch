  <env name="ROS_LOG_DIR" value="/tmp/"/>
  
  <include file="$(find wheel_bridge)/launch/wheel_front_left.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel_front_right.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel_back_left.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel_back_right.launch">
  </include>

</launch>
