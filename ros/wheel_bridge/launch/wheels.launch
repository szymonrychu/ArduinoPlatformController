<launch>
  <env name="ROSCONSOLE_FORMAT" value="[${severity}][${thread}][${node}/${function}:${line}]: ${message}"/>
  <env name="ROS_LOG_DIR" value="/tmp/"/>

  <node pkg="tf" type="static_transform_publisher" name="wheel1_translation" args=" 0.23625   0.13  0.0575  0 0 0 /base_link /wheel1_base 50"/>
  <node pkg="tf" type="static_transform_publisher" name="wheel2_translation" args=" 0.23625  -0.13  0.0575  0 0 0 /base_link /wheel2_base 50"/>
  <node pkg="tf" type="static_transform_publisher" name="wheel3_translation" args="-0.23625   0.13  0.0575  0 0 0 /base_link /wheel3_base 50"/>
  <node pkg="tf" type="static_transform_publisher" name="wheel4_translation" args="-0.23625  -0.13  0.0575  0 0 0 /base_link /wheel4_base 50"/>
  
  <include file="$(find wheel_bridge)/launch/wheel1.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel2.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel3.launch">
  </include>
  <include file="$(find wheel_bridge)/launch/wheel4.launch">
  </include>

</launch>
